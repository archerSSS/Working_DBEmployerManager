<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="DBEmployerManager"
				   namespace="DBEmployerManager.Models">
	<class name="Employers">
		<id name="Id" column="Id">
			<generator class="identity"></generator>
		</id>
		<property name="FirstName" column="FirstName"></property>
		<property name="LastName" column="LastName"></property>
		<property name="Patronymic" column="Patronymic"></property>
		<property name="Born" column="Born"></property>
		<property name="Gender" column="Gender"></property>
		<property name="UnitName" column="UnitName"></property>
	</class>

</hibernate-mapping>
